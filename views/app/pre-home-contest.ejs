    <script>
      function resdirectAfterLogin(redirectUrl) {
        // Ensure this value is correctly populated
        var cookieValue = encodeURIComponent(redirectUrl);
        var cookieString =
          "resUrl=" + cookieValue + "; path=/; Secure; SameSite=None;";

        // Set or overwrite the 'resUrl' cookie
        document.cookie = cookieString;

        // Redirect to login page
        window.location.href = "/login";
      }
